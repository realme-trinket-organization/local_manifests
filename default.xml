    <remote name="rtrinket"
        fetch="ssh://git@github.com/realme-trinket-organization/"
        revision="lineage-19.1" />
    
    <!-- Caf Component -->
    <remote name="mario"
        fetch="https://github.com/ThankYouMario"
        revision="sapphire" />
    <remote name="aospa"
        fetch="https://github.com/AOSPA"
        revision="sapphire" />

    <!-- Device Specific -->
    <project path="device/realme/realme_trinket" name="device_realme_trinket" remote="rtrinket" />
