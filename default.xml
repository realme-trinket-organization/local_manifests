    <project path="vendor/realme/realme_trinket" name="vendor_realme_trinket" remote="rtrinket" />

    <!-- Caf Component -->
    <project name="android_device_qcom_common" path="device/qcom/common" remote="aospa" clone-depth="1" />
    <project name="proprietary_vendor_qcom_common" path="vendor/qcom/common" remote="mario" clone-depth="1" />
</manifest>
